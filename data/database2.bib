@ARTICLE{Xue20241798,
	author = {Xue, Yu and Fan, Runchuan and Ding, Aizhong and Yang, Kai},
	title = {Research Progress on Chelating Agent Induced Phytoremediation of Uranium-Contaminated Soils; [螯合诱导植物修复铀污染土壤的研究进展]},
	year = {2024},
