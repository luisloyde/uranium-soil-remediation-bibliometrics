@ARTICLE{Radenković200861,
	author = {Radenković, Mirjana B. and Cupać, Svjetlana A. and Joksić, Jasminka D. and Todorović, Dragana J.},
	title = {Depleted uranium mobility and fractionation in contaminated soil (Southern Serbia)},
