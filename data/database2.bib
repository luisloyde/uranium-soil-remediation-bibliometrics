@ARTICLE{Cleveland2021,
	author = {Cleveland, Danielle and Hinck, Jo Ellen and Lankton, Julia S.},
	title = {Elemental and radionuclide exposures and uptakes by small rodents, invertebrates, and vegetation at active and post-production uranium mines in the Grand Canyon watershed},
	year = {2021},
	journal = {Chemosphere},
	volume = {263},
	doi = {10.1016/j.chemosphere.2020.127908},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85089701549&doi=10.1016%2fj.chemosphere.2020.127908&partnerID=40&md5=4ca613a7dd8c0a51d6399c396abb21c4},
