@BOOK{Dragović2012203,
	author = {Dragović, Snežana and Ćujić, Mirjana and Kovačević, Jovan},
	title = {Environmental radiological impact of uranium mining and milling operations},
	year = {2012},
	journal = {Radionuclides: Sources, Properties and Hazards},
	pages = {203 – 233},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84895207223&partnerID=40&md5=9660a03f84da4008f3b4f15411b48355},
	affiliations = {University of Belgrade, Institute for the Application of Nuclear Energy, Belgrade, Serbia; Geological Institute of Serbia, Belgrade, Serbia},
	abstract = {As an integral part of nuclear fuel cycle, uranium mining and milling operations are considered to potentially be a significant source of environmental contamination in and around mining facilities. In addition to radionuclides of uranium and thorium decay series, uranium mill tailings are often associated with elevated concentrations of highly toxic heavy metals. The metal and radionuclide contaminated solid residues scattered in uranium mining areas contribute to an extensive contamination of the soil posing the serious risks to living organisms of surrounding areas. The contaminants from waste and tailing dumps in uranium mining and processing sites pose contamination risks to surface water and groundwater. In tailings resulting from acid leaching, the mobilization of radionuclides, metals and metalloids from solid into liquid phase may occur after tailings deposition, even long after rehabilitation of the tailings repository has been completed. Even when mining activities are ceased, these installations continue to have an impact to the environment due to the surface exposure of materials rich in uranium and its radioactive descendants. Currently, many countries have legacy of abandoned uranium mining sites with large amounts of mining waste in surface disposal grounds and remediation actions are being considered for many abandoned sites. In this chapter the risks and hazards associated with radioactive contamination at active and abandoned uranium mining sites and surrounding areas are reviewed. The results of the radiological assessment study conducted in the largest uranium mineralized area in Serbia, where abandoned uranium mines are situated, are presented. © 2012 Nova Science Publishers, Inc. All rights reserved.},
	correspondence_address = {S. Dragović; University of Belgrade, Institute for the Application of Nuclear Energy, Belgrade, Serbia; email: sdragovic@inep.co.rs},
	publisher = {Nova Science Publishers, Inc.},
	isbn = {978-161942748-8},
	language = {English},
	abbrev_source_title = {Radionuclides: Sources, Prop. and Hazards},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0}
}

