@ARTICLE{Gu20033575,
	author = {Gu, Baohua and Chen, Jie},
	title = {Enhanced microbial reduction of Cr(VI) and U(VI) by different natural organic matter fractions},
	year = {2003},
	journal = {Geochimica et Cosmochimica Acta},
	volume = {67},
	number = {19},
