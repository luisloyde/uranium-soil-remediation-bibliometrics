	doi = {10.1097/HP.0b013e3182732c73},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84871882185&doi=10.1097%2fHP.0b013e3182732c73&partnerID=40&md5=b3c0bb623ee0301b9bf39d0c47911a50},
