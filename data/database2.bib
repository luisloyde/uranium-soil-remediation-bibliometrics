@CONFERENCE{Miller20051656,
	author = {Miller, David S. and Quinn, John J. and Johnson, Robert L.},
	title = {Determining the appropriate mix of statistical tools to guide characterization, remediation, and closure at a uranium-contaminated site},
	year = {2005},
	journal = {Proceedings - 10th International Conference on Environmental Remediation and Radioactive Waste Management, ICEM'05},
	volume = {2005},
	pages = {1656 – 1661},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-33646594738&partnerID=40&md5=8c9ff1ad38cdde81eaf02f3523f86797},
	affiliations = {Argonne National Laboratory, Argonne, IL, United States},
	abstract = {From the early 1940s to the early 1950s, various forms of uranium were processed at a site in the Midwestern United States. Based on gamma walkover survey results, uranium is concentrated near the primary process building, and excavation is necessary. However, contamination is much more sparsely distributed over large, more remote areas of the site. A cleanup criterion was established for determining whether the remote areas are likely to pass closure protocol established using the Multi-Agency Radiation Survey and Site Investigation Manual (MARSSIM). Two key issues were considered as part of the characterization strategy. Because it is expensive to dispose of radiologically contaminated soil, the degree of uncertainty about the projected volume of contaminated soil is a critical factor in estimating the cost and schedule of the remediation. The second issue is determining whether the large, remote areas are likely to meet closure criteria or require remediation. Three statistical techniques were considered as tools to be used to guide characterization activities and to quantify the important elements of decision uncertainty. Their utility and applicability depended upon the spatial characteristics of the contamination and the decisions to be made. Variograms, geostatistical tools that determine the increasing variance with increasing data separation distance, were used. The parametric geostatistical approach resulted in poorly supported variograms that did not show a clear spatial correlation near the processing building, indicating that kriging of concentration values was inappropriate for resolving excavation boundaries. Indicator geostatistics were then considered. Applying a binary perspective to the data (i.e., a point is either above or below the cleanup criterion) provided an indicator variogram that was far more robust than that obtained with the parametric data. The spatial correlation observed using the indicator approach provides a more technically defensible delineation of excavation footprints, and kriging of indicator values provides a means of quantifying and evaluating uncertainty by measuring the relationship between contaminated area (or volume) and contamination probability. For the larger, remote areas where uranium concentrations are low and there is no spatial correlation between sampling points, the approach is to use MARSSIM, which is based on a classical statistical approach already well-accepted in the regulatory community. The goal is to evaluate each survey unit's potential of meeting the closure criteria without remediation. If a unit fails to meet the criteria, the offending portion is isolated and subject to a denser sampling scheme or designated for remediation. This case study demonstrates how a variety of statistical tools were applied to two distinct regions of a site. Our experiences have shown that parametric geostatistics generally fail in determining spatial correlation in anthropogenic contamination; indicator geostatistics provide a technically defensible estimate of areas or volumes that are above or below a specified cleanup criterion, for instance in determining the footprint of soil to be excavated; and classical statistics are appropriate for closeout of broad areas with low levels of contamination. Copyright © 2005 by ASME.},
	keywords = {Contamination; Cost accounting; Excavation; Land reclamation; Parameter estimation; Regulatory compliance; Soil pollution; Contaminated area; Contamination probability; Geostatistical tools; Uranium-contaminated site; Radioactive elements},
	language = {English},
	abbrev_source_title = {Proc. Int. Conf. Environ. Remediation Radioactive Waste Manage.},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 2; Conference name: 10th International Conference on Environmental Remediation and Radioactive Waste Management, ICEM'05; Conference date: 4 September 2005 through 8 September 2005; Conference code: 67275}
}

