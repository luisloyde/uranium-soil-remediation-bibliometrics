@ARTICLE{Prasad2003110,
	author = {Prasad, Majeti Narasimha Vara and De Oliveira Freitas, Helena Maria},
	title = {Metal hyperaccumulation in plants - Biodiversity prospecting forphytoremediation technology},
	year = {2003},
	journal = {Electronic Journal of Biotechnology},
	volume = {6},
