@BOOK{Munazir2022423,
	author = {Munazir, Mehmooda and Qureshi, Rahmatullah and Munir, Mubashrah and Mukhtar, Hamid},
	title = {Role of Phytoremediation as a Promising Technology to Combat Environmental Pollution},
	year = {2022},
	journal = {Phytoremediation for Environmental Sustainability},
