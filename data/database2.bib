@CONFERENCE{Ebbs2000466,
	author = {Ebbs, Stephen and Brady, Danielle and Norvell, Wendell and Kochian, Leon},
	title = {Uranium speciation, plant uptake, and phytoremediation},
	year = {2000},
	journal = {Environmental and Pipeline Engineering 2000},
	pages = {466 – 475},
	doi = {10.1061/40507(282)51},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-0034587646&doi=10.1061%2f40507%28282%2951&partnerID=40&md5=34a30d58b73308bd9f6128b3d59d52d2},
	affiliations = {Department of Plant Biology, Southern Illinois Univ.-Carbondale, Carbondale, IL 62901, United States},
	abstract = {The selection of plant species and amendments for the phytoremediation of uranium (U) contaminated soil has been hampered by a lack of information relating the speciation of U in the soil to plant uptake. The goals of the present study were to: 1) provide fundamental information regarding the uptake and translocation of U by plants; 2) utilize that information to improve the phytoextraction of U from contaminated soil. The first goal was achieved by using computer speciation modeling and hydroponic experiments to demonstrate that the uranyl (UO22+) cation was the U species most readily accumulated in plant shoots. A hydroponic screening experiment was then conducted to identify plants species capable of accumulating the uranyl cation in plant shoots. A soil incubation study addressed the second goal by examining the solubilization of U from contaminated soil by synthetic chelates (HEDTA) and organic acids (citric, oxalic). The results indicated that citric acid solubilized >100 times more U than the other amendments. The results of the hydroponic and soil experiments were then integrated in a pot study in which red beet was grown in U-contaminated soil amended with either citric acid or HEDTA. Citric acid was again highly effective, dramatically increasing shoot U content. In addition to providing fundamental information regarding the form of U taken up by plants, this study also illustrates the importance of basic research to the field of phytoremediation. Based on this information and other studies we have conducted, it should be possible to develop an effective phytoremediation strategy for U-contaminated soils, using citric acid as the principle soil amendment.},
	keywords = {Computer simulation; Contamination; Extraction; Organic acids; Plants (botany); Soil pollution; Solubility; Uranium; Citric acid; Phytoextraction; Phytoremediation; Plant uptake; Synthetic chelates; Uranium speciation; Bioremediation},
	editor = {Surampalli R.Y.},
	publisher = {ASCE - American Society of Civil Engineers},
	isbn = {0784405077; 978-078440507-9},
	language = {English},
	abbrev_source_title = {Environ. Pipel. Eng. 2002},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 2; Conference name: Proceedings of the ASCE National Conference on Environmental and Pipeline Engineering 2000; Conference date: 23 July 2000 through 26 July 2000; Conference code: 59964}
}

@ARTICLE{Elless1997716,
