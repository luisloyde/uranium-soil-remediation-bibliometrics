	note = {Cited by: 3}
}

@ARTICLE{Prieto2013439,
	author = {Prieto, C. and Lozano, J.C. and Rodríguez, P. Blanco and Tomé, F.Vera},
	title = {Enhancing radium solubilization in soils by citrate, EDTA, and EDDS chelating amendments},
