	year = {2011},
	journal = {Environmental Science and Technology},
	volume = {45},
	number = {4},
	pages = {1250 – 1256},
	doi = {10.1021/es102893r},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-79951634614&doi=10.1021%2fes102893r&partnerID=40&md5=648b373d1d96e78f830ac14c294f10fb},
