@BOOK{Gramss201521,
	author = {Gramss, Gerhard and Voigt, Klaus-Dieter},
	title = {The legacy of uranium mining: Farming on non-remediable metalliferous soils?},
	year = {2015},
	journal = {Uranium: Sources, Exposure and Environmental Effects},
	pages = {21 – 52},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84958701179&partnerID=40&md5=49ddb42c56b4bce4f77e0367382dfdd4},
	affiliations = {Institute of Geosciences, Friedrich-Schiller-University, Jena, Germany; Food GmbH Jena, Jena, Germany},
	abstract = {The production of 251,000 Mg uranium by the Soviet-East-German mining company SDAG Wismut from 1946 to 1990 based on ore deposits in the densely populated regions of Ronneburg and Seelingstädt in Thuringia and at numerous settlements in Saxony. The activities extending over 100 km2 depreciated 34 km2 with waste rock dumps and ore mill tailings, open pits, more than 50 mine shafts, and 30 drift systems up to 2,900 km in length. Recontouring and restoration of the radionuclide and heavy metal (HM) contaminated sites with an expense of 180 Mio € km-2 was financed by a public fund. Covered by up to 1 m casing soil, the less contaminated sites are afforested, turfed, and stigmatized by their history and the lasting HM release into water systems. They must nevertheless sometimes be returned to productive use. Respective case studies substantiated thus environmental long-term damages inflicted on "reclaimed" subsoil strata at Gessenwiese by infiltrations from onsite ore leaching which ended 15 years ago. Furthermore, grass- and arable lands formed in 1965 from overburden dumps at Seelingstädt were estimated for the compatibility of soils and crops with common hygiene standards. The Corg-deficient sandy Gessenwiese soils pH 3.9-5.2 matched with the usual and permissible HM concentration ranges set for arable land but contaminated the groundwater with CdNiU. The dominance of the highly plant-available free cations and metal-sulphate complexes in the wide absence of organic ligands in the soil solution made four crop plants elevated in MnNiUZn and exceeding the limit of 1 mg kg-1 DW set for Cd in forage. The re-cultured and turfed clay-loam soils pH 6.6-7.2 around Seelingstädt were moderately elevated in AsCdCuZn. Thirty years' applications of composted and liquid husbandry manure raised the humus content to 34 % in the 0-3-cm sward layer, lowered HM concentrations by the same amount, and made the mix of green forage concur with EU hygiene standards. Use of the grasslands as pasturage, e.g., for sheep should nevertheless be confined from Jan to March due to high ingestion rates of roots with the adhering metalliferous soils. Local HM hot spots of arable land were found severely elevated in AsCuMnPbUZn and reached Cd concentrations of 28-40 mg kg-1. Envisaged forage crops of grasses, cereals, and legumes were with Cd concentrations of 4-11 mg kg-1 DW not acceptable. In contrast, seed crops of pea, rye, and an excluder cv. of wheat concurred safely with EU standards of AsPb in food and for Cd in forage and tolerated soil concentrations in AsCuMnUZn 2 to 5 times those currently regarded as permissible. Identical traits denoted HM distributions in the vertical soil profiles of both test sites. In two Gessenwiese test pits, concentrations of 12/4 elements in strata 40-80 cm deep were 29/43 % lower than those at 160-200 cm and could be seen as the outcome of 15 years' relocations. The contemporary annual rates of metal export by test plants and by downwash with seepage water from the total content of a 1-m soil layer remained with 0.03/0.13 % in the negligible range. In 1-m soil profiles at Seelingstädt, HM concentrations in the 4-20-cm (plough) layer of grassland were by 16/52 % significantly lower than mean values determined for the 1-m soil horizon due to apparent relocations of 35 years. Phytoextraction claims thousands to ten thousands of years for comparable rates of HM export. It is concluded that the plough layer of moderately contaminated soils can apparently more be disburdened by downward motions of metal-associated colloids referred to as clay migration rather than by the common mass flow of metals (complexes) and by flopping phytoextractions in practicable time spans of 30-100 years. Seed crops with their out-regulated HM balance recommend themselves for fodder production on non-remediable soils elevated especially in Cd. They tolerate soil HM concentrations beyond the currently permissible limits and reduce the pressure on the quality of soil remediations. © 2015 by Nova Science Publishers, Inc. All rights reserved.},
	author_keywords = {Clay migration; Downwash; Excluder seed crops; Heavy metal; Land reclamation costs; Non-remediable metalliferous soil; Pasturage; Phytoextraction; Radionuclide},
	correspondence_address = {G. Gramss; Institute of Geosciences, Friedrich-Schiller-University, Jena, Germany; email: Gerhard.gramss@uni-jena.de},
	publisher = {Nova Science Publishers, Inc.},
