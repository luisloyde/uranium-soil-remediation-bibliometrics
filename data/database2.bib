@ARTICLE{Cummings19961,
	author = {Cummings, Mark and Booth, Steven R.},
	title = {Remediation of uranium-contaminated soil using the segmented gate system and containerized vat leaching techniques: A cost-effectiveness study},
	year = {1996},
	journal = {Remediation},
	volume = {7},
