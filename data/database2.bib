	publisher = {Nova Science Publishers, Inc.},
	isbn = {978-162081207-5},
	language = {English},
	abbrev_source_title = {Uranium: Charact., Occurrence and Hum. Expos.},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0}
}

