@CONFERENCE{Torma199371,
	author = {Torma, Arpad E.},
	title = {Multi-disciplinary approaches for environmentally safe processing of materials for properties},
	year = {1993},
	journal = {First International Conference on Processing Materials for Properties},
	pages = {71 – 76},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-0027841747&partnerID=40&md5=03c79955075e29f138bf552e2cf2f825},
	affiliations = {EG&G Idaho Inc., Idaho, United States},
	abstract = {In the last decade, the application of biotechnological principles in the processing of low-grade mineral materials, which are economically not amenable for metal production by the conventional technology, is gaining considerable industrial importance. This tendency is especially motivated by the fact that biotechnological processes are simple to operate, less capital and energy extensive than the conventional technologies. Furthermore, these processes are environment friendly since they do not result any air, soil or aquifer pollution. As a rule of thumb, for example, a pound of copper can be produced from mining wastes for about one-third to one-half of the costs of copper production by the conventional smelting process from high-grade sulfide concentrate. Currently, approximately 25% of the total copper produced in the United States is imputable to bacterial activity in heap, dump and in-situ leaching operations. In Canada, an important segment of the uranium production is achieved by bio-assisted leach technologies. Other industrial application of biotechnological processing is related to gold production from refractory pyrite and arsenopyrite materials. When submicron-size native gold particles are finely disseminated within the above refractory ore materials, then the conventional cyanidation technology becomes economically marginal for the gold production. However, if these materials are subjected to pre-oxidation by the bacteria, pores will be created in the mineral matrices and the follow-up cyanidation can successfully extract gold. This integrated biopre-oxidation and cyanidation process is industrially practiced in South Africa, Brazil and Australia. Further establishment of industrial plants are in progress in Ghana. diverse bioremediation processes are under development for the removal of trace concentrations of heavy metals and radionuclides from very large volumes of industrial effluents and contaminated soils. It is well known that the conventional physical and chemical technologies are economically handicapped for dealing with these situations.},
	keywords = {Copper; Metallurgy; Processing; Bio processing of materials; Environmentally safe; Low grade mineral materials; Multi disciplinary approaches; Biotechnology},
	publisher = {Publ by Minerals, Metals & Materials Soc (TMS)},
	isbn = {0873392566},
	language = {English},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0; Conference name: Proceedings of the 1st International Conference on Processing Materials for Properties; Conference date: 7 November 1993 through 10 November 1993; Conference code: 19624}
}

