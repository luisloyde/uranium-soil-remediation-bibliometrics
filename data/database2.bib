}

@BOOK{Gramss2010361,
	author = {Gramss, Gerhard and Bergmann, Hans},
	title = {Optimizing the content of nutritionally essential trace metals in Chinese cabbage (Brassica chinensis l.) with soil applications of compost or nitrogen},
	year = {2010},
	journal = {Agriculture Research and Technology},
	pages = {361 – 378},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85049409823&partnerID=40&md5=a9629867c0145d3d15e6416e7ca055df},
	affiliations = {Institute of Geological Sciences, Friedrich-Schiller-University, Burgweg 11, Jena, D-07743, Germany},
	abstract = {Applications of N, P, and K suffice in general to obtain crops rich in biomass, protein, and in those trace metals which are essential for plant's and consumer's health. Whereas trace metal deficiencies in soils can be easily overcome, seriously metalliferous soils lie waste in expectance of cost-efficient remediation treatments. It was recently shown that uptake of the metalloproteid-associated transition metals, (Cd), Co, Cu. Mn, Ni, and Zn as well as Ca and Mg rises and falls near-linearly with the Norg content in dicots. Moreover, traces of phytochelants and certain amino acids in soil trigger a disproportionate uptake of selected elements (Co, Mn), while other amino compounds support dry weight and Norg formation but not the (proportionate) uptake of trace metals. Applications of trace metals to deficient soils or of the soil-N consume initiating hydrocarbons to metalliferous soils to repress Norg formation and (heavy) metal uptake synchronously are therefore not the only ways to optimize the trace metal content of crop plants. In the present study, Chinese cabbage was potted on extremely metalliferous uranium mine dump soil substrates. The original soil Corg 2.5 % (w/w) was attenuated with quartz sand (Corg = 0) to substrates Corg 0.5 and 1.55 %. It was also enriched with Fischer's commercial compost (Corg 21 %) to substrates Corg 6.6 and 9.5 %. The soil was treated in this way to lower the concentrations of hazardous elements in the soil solution by simple attenuation or by sorption to organic matter and, thus, to restrict plant uptake of heavy metals. Compost was also expected to serve as a "hydrocarbon" supplement with effects on the plant's Norg formation and metal homeostasis. Due to the largely differing mineral-N resources from sand-attenuated to the compost-amended soil substrates, dry weight production (5.5 times) and Norg content (3.3 times) of Chinese cabbage shoots varied considerably. In shoot tissues of plants from soil samples Corg 0.5 to 2.5 %, concentrations of the proteid-associated transition metals and of Ca and Mg fell and rose once more near-linearly with Norg. Uptake of certain essential and non-essential elements declined only in the case of plants from soil Corg 0.5 % with its seriously cut metal content. With the introduction of Fischer's compost, the relation between the content in shoot Norg and a proportionate uptake in the sum of the proteid-associated transition metals and in Ca and Mg was dramatically reversed, although these metals remained in solution and did not bind to the compost supplement. Shoot tissues of plants from soil samples Corg 6.6/9.5 % did not form top concentrations in Norg. They contained nevertheless as little as 28/19 % from the quantity of the five transition metals which would theoretically be taken up from non-amended soil to correspond with the quantity of Norg. Uptake of Ca and Mg was also repressed, whereas uptake of non-essential elements was almost not impaired by soil Corg and shoot Norg. Among several other sources of commercial biosolids, Zeus compost (Corg 42 %) restricted the formation of shoot Norg upon the N-consuming degradation of its inherent hexose sugars. The compost did not disrupt the proportionality between concentrations in shoot Norg and in Ca, (Cd), Mg, Ni, and Zn, whereas uptake of Co, Cu, and Mn was selectively repressed. It is concluded that soils contaminated with (Cd), Co, Cu, Mn, Ni, and Zn but not with other hazardous trace metals could be opened to crop production via application of certain biosolids and a restricted N supply. Crop production on trace metal deficient soils should imply the application of soil acidifying and the formation of plant protein promoting NH4 fertilizers as well as of organic N sources which are mineralized via NH3/NH4+ intermediates. Application of organic carbon should be confined. It is further concluded that changes in the plant's metal homeostasis in the presence of several composts point to a potential role of those microbial plant growth hormones which had been formed during its industrial fermentation. Compost may also serve as a hydrocarbon-like supplement with soil-N sequestration inducing and thus plant Norg and metal uptake restricting properties. © 2011 by Nova Science Publishers, Inc. All rights reserved.},
