	correspondence_address = {N. Perdrial; Department of Environmental Science, University of Arizona, Tucson, 1177 East Fourth Street, 85721, United States; email: nperdria@uvm.edu},
	publisher = {Elsevier Ltd},
	issn = {0265931X},
	coden = {JERAE},
	pmid = {35772319},
	language = {English},
	abbrev_source_title = {J. Environ. Radioact.},
