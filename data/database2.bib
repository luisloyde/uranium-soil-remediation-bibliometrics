@ARTICLE{Tokunaga20095467,
	author = {Tokunaga, Tetsu K. and Kim, Yongman and Wan, Jiamin},
	title = {Potential remediation approach for uranium-contaminated groundwaters through potassium uranyl vanadate precipitation},
